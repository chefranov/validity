{
	"name": "__MSG_extn_name__",
	"version": "@version@",
	"default_locale": "en_GB",
	"minimum_chrome_version": "58",
	"description": "__MSG_extn_description__",
	"icons": {
		"16": "icons/16.png",
		"32": "icons/32.png",
		"48": "icons/48.png",
		"64": "icons/64.png",
		"128": "icons/128.png"
	},
	"permissions": [
		"http://*/*",
		"https://*/*",
		"tabs",
		"storage"
	],
	"content_security_policy": "script-src 'self' https://ssl.google-analytics.com; object-src 'self'",
	"background": {
		"scripts": [
      "**/*.js"
		]
	},
	"manifest_version": 2
}
